{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "paths": {
      "@barca/shared": [
        "data_visual/shared"
      ],
      "@barca/shared/*": [
        "data_visual/shared/*"
      ],
      "@shared": [
        "app/shared","designer/shared"
      ],
      "@shared/*": [
        "app/shared/*","designer/shared/*"
      ],
      "@core": [
        "app/core","designer/core"
      ],
      "@core/*": [
        "app/core/*", "designer/core/*"
      ],
      "@testing": [
        "testing"
      ],
      "@testing/*": [
        "testing/*"
      ],
      "@env": [
        "environments"
      ],
      "@env/*": [
        "environments/*"
      ]
    },
    "baseUrl": "./"
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
